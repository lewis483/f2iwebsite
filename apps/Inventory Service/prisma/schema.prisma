datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Inventory {
  createdAt         DateTime @default(now())
  id                String   @id @default(cuid())
  productId         String?
  quantityAvailable Int?
  updatedAt         DateTime @updatedAt
}
